                                  [(CSRElem timeStep varNo (- 1))])                                                                                                                                     {--)--}
                                                                                                                                                                                                        {--)--}
    WriteStatementMulLiteral : (timeStep : Nat) ->                                                                                                                                                      {--)--}
                               (varNo : Nat) ->                                                                                                                                                         {--)--}
                               (leftDigit : Integer) ->                                                                                                                                                 {--)--}
                               (rightVarNo : Nat) ->                                                                                                                                                    {--)--}
                               (StatementWritten                                                                                                                                                        {--)--}
                                 timeStep                                                                                                                                                               {--)--}
                                 (Operation                                                                                                                                                             {--)--}
                                   varNo MUL (DigitLeaf leftDigit) (VarLeaf rightVarNo))                                                                                                                {--)--}
                               [(ConstElem timeStep leftDigit)]                                                                                                                                         {--)--}
                               [(CSRElem timeStep rightVarNo 1)]                                                                                                                                        {--)--}
                               [(CSRElem timeStep varNo (- 1))])                                                                                                                                        {--)--}
                                                                                                                                                                                                        {--)--}
    WriteStatementMulLiteral' : (timeStep : Nat) ->                                                                                                                                                     {--)--}
                                (varNo : Nat) ->                                                                                                                                                        {--)--}
                                (leftVarNo : Nat) ->                                                                                                                                                    {--)--}
                                (rightDigit : Integer) ->                                                                                                                                               {--)--}
                                (StatementWritten                                                                                                                                                       {--)--}
                                  timeStep                                                                                                                                                              {--)--}
                                  (Operation                                                                                                                                                            {--)--}
                                    varNo MUL (VarLeaf leftVarNo) (DigitLeaf rightDigit))                                                                                                               {--)--}
                                [(CSRElem timeStep leftVarNo 1)]                                                                                                                                        {--)--}
                                [(ConstElem timeStep rightDigit)]                                                                                                                                       {--)--}
                                [(CSRElem timeStep varNo (- 1))])                                                                                                                                       {--)--}
                                                                                                                                                                                                        {--)--}
    WriteAsgn : (timeStep : Nat) ->                                                                                                                                                                     {--)--}
                (varNo : Nat) ->                                                                                                                                                                        {--)--}
                (leftVarNo : Nat) ->                                                                                                                                                                    {--)--}
                (StatementWritten                                                                                                                                                                       {--)--}
                    timeStep                                                                                                                                                                            {--)--}
                    (Operation                                                                                                                                                                          {--)--}
                        varNo ASGN (VarLeaf leftVarNo) NilLeaf)                                                                                                                                         {--)--}
                    [(CSRElem timeStep leftVarNo 1)]                                                                                                                                                    {--)--}
                    [(ConstElem timeStep 1)]                                                                                                                                                            {--)--}
                    [(CSRElem timeStep varNo (- 1))])                                                                                                                                                   {--)--}
                                                                                                                                                                                                        {--)--}
    WriteAsgn' : (timeStep : Nat) ->                                                                                                                                                                    {--)--}
                 (varNo : Nat) ->                                                                                                                                                                       {--)--}
                 (leftVal : Integer) ->                                                                                                                                                                 {--)--}
                 (StatementWritten                                                                                                                                                                      {--)--}
                    timeStep                                                                                                                                                                            {--)--}
                    (Operation                                                                                                                                                                          {--)--}
                        varNo DIGIT (DigitLeaf leftVal) NilLeaf)                                                                                                                                        {--)--}
                    [(ConstElem timeStep leftVal)]                                                                                                                                                      {--)--}
                    [(ConstElem timeStep 1)]                                                                                                                                                            {--)--}
                    [(CSRElem timeStep varNo (- 1))])                                                                                                                                                   {--)--}
                                                                                                                                                                                                        {--)--}
    WriteStatementMulInt : (timeStep : Nat) ->                                                                                                                                                          {--)--}
                            (varNo : Nat) ->                                                                                                                                                            {--)--}
                            (leftDigit : Integer) ->                                                                                                                                                    {--)--}
                            (rightDigit : Integer) ->                                                                                                                                                   {--)--}
                            (StatementWritten                                                                                                                                                           {--)--}
                                timeStep                                                                                                                                                                {--)--}
                                (Operation                                                                                                                                                              {--)--}
                                    varNo MUL (DigitLeaf leftDigit) (DigitLeaf rightDigit))                                                                                                             {--)--}
                                [(ConstElem timeStep leftDigit)]                                                                                                                                        {--)--}
                                [(ConstElem timeStep rightDigit)]                                                                                                                                       {--)--}
                                [(CSRElem timeStep varNo (- 1))])                                                                                                                                       {--)--}
