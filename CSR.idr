data CSRList : (timeStep : Nat) ->                                                                                                                                                                      {--)--}
               (statements : List R1CSStatement) ->                                                                                                                                                     {--)--}
               (lLines : List CSRLine) ->                                                                                                                                                               {--)--}
               (rLines : List CSRLine) ->                                                                                                                                                               {--)--}
               (cLines : List CSRLine) ->                                                                                                                                                               {--)--}
               Type where                                                                                                                                                                               {--)--}
    WriteStatements : (timeStep : Nat) ->                                                                                                                                                               {--)--}
                      (headStmt : R1CSStatement) ->                                                                                                                                                     {--)--}
                      (remainingStmts : List R1CSStatement) ->                                                                                                                                          {--)--}
                      (headStmtWritten : StatementWritten timeStep headStmt lLines rLines cLines) ->                                                                                                    {--)--}
                      (remainingStmtsWritten : CSRList (S timeStep) remainingStmts lLLines rLLines cLLines) ->                                                                                          {--)--}
                      CSRList timeStep (headStmt :: remainingStmts) (lLines ++ lLLines) (rLines ++ rLLines) (cLines ++ cLLines)                                                                         {--)--}
                                                                                                                                                                                                        {--)--}
    WriteEmptyStatements : (timeStep : Nat) ->                                                                                                                                                          {--)--}
                           (CSRList timeStep [] [] [] [])                                                                                                                                               {--)--}
                                                                                                                                                                                                        {--)--}
data CSRListContainer : (timeStep : Nat) -> (statements : List R1CSStatement) -> Type where                                                                                                             {--)--}
    MkCSRListContaner : (timeStep : Nat) ->                                                                                                                                                             {--)--}
                        (statements : List R1CSStatement) ->                                                                                                                                            {--)--}
                        (lLines : List CSRLine) ->                                                                                                                                                      {--)--}
                        (rLines : List CSRLine) ->                                                                                                                                                      {--)--}
                        (cLines : List CSRLine) ->                                                                                                                                                      {--)--}
                        (writtenPrf : CSRList timeStep statements lLines rLines cLines) ->                                                                                                              {--)--}
                        CSRListContainer timeStep statements                                                                                                                                            {--)--}
                                                                                                                                                                                                        {--)--}
                                                                                                                                                                                                        {--)--}
cantWriteRemaining : (contra : CSRListContainer (S timeStep) xs -> Void) ->                                                                                                                             {--)--}
                     (theStatement : StatementWritten timeStep x headLLines headRLines headCLines) ->                                                                                                   {--)--}
                     CSRListContainer timeStep (x :: xs) -> Void                                                                                                                                        {--)--}
cantWriteRemaining contra                                                                                                                                                                               {--)--}
                   theStatement                                                                                                                                                                         {--)--}
                   (MkCSRListContaner timeStep                                                                                                                                                          {--)--}
                                      (x :: xs)                                                                                                                                                         {--)--}
                                      (ys ++ lLLines)                                                                                                                                                   {--)--}
                                      (zs ++ rLLines)                                                                                                                                                   {--)--}
                                      (ws ++ cLLines)                                                                                                                                                   {--)--}
                                      (WriteStatements timeStep x xs headStmtWritten remainingStmtsWritten)) =                                                                                          {--)--}
    let container = (MkCSRListContaner (S timeStep) xs lLLines rLLines cLLines remainingStmtsWritten) in                                                                                                {--)--}
        contra container                                                                                                                                                                                {--)--}
                                                                                                                                                                                                        {--)--}
cantWriteHead : (headContra : StatementWrittenContainer timeStep x -> Void) ->                                                                                                                          {--)--}
                CSRListContainer timeStep (x :: xs) ->                                                                                                                                                  {--)--}
                Void                                                                                                                                                                                    {--)--}
cantWriteHead headContra                                                                                                                                                                                {--)--}
              (MkCSRListContaner timeStep (x :: xs) (ys ++ lLLines) (zs ++ rLLines) (ws ++ cLLines)                                                                                                     {--)--}
              (WriteStatements timeStep x xs headStmtWritten remainingStmtsWritten)) =                                                                                                                  {--)--}
    let container = (MkContainer timeStep x ys zs ws headStmtWritten) in                                                                                                                                {--)--}
        headContra container                                                                                                                                                                            {--)--}
                                                                                                                                                                                                        {--)--}
writeCSRList : (timeStep : Nat) -> (statements : List R1CSStatement) -> Dec (CSRListContainer timeStep statements)                                                                                      {--)--}
writeCSRList timeStep [] = Yes (MkCSRListContaner timeStep [] [] [] [] (WriteEmptyStatements timeStep))                                                                                                 {--)--}
writeCSRList timeStep (x :: xs) = case (writeStatement timeStep x) of                                                                                                                                   {--)--}
    (Yes (MkContainer timeStep x headLLines headRLines headCLines theStatement)) =>                                                                                                                     {--)--}
        case (writeCSRList (S timeStep) xs) of                                                                                                                                                          {--)--}
            (Yes                                                                                                                                                                                        {--)--}
                (MkCSRListContaner                                                                                                                                                                      {--)--}
                    (S timeStep)                                                                                                                                                                        {--)--}
                    (headStmt :: remainingStmts)                                                                                                                                                        {--)--}
                    (ys ++ lLLines)                                                                                                                                                                     {--)--}
                    (zs ++ rLLines)                                                                                                                                                                     {--)--}
                    (ws ++ cLLines)                                                                                                                                                                     {--)--}
                    (WriteStatements                                                                                                                                                                    {--)--}
                        (S timeStep)                                                                                                                                                                    {--)--}
                        headStmt                                                                                                                                                                        {--)--}
                        remainingStmts                                                                                                                                                                  {--)--}
                        headStmtWritten                                                                                                                                                                 {--)--}
                        remainingStmtsWritten))) =>                                                                                                                                                     {--)--}
                let fullPrf = (WriteStatements                                                                                                                                                          {--)--}
                                timeStep                                                                                                                                                                {--)--}
                                x                                                                                                                                                                       {--)--}
                                (headStmt :: remainingStmts)                                                                                                                                            {--)--}
                                theStatement                                                                                                                                                            {--)--}
                                (WriteStatements (S timeStep) headStmt remainingStmts headStmtWritten remainingStmtsWritten)) in                                                                        {--)--}
                    Yes                                                                                                                                                                                 {--)--}
                        (MkCSRListContaner                                                                                                                                                              {--)--}
                            timeStep                                                                                                                                                                    {--)--}
                            (x :: (headStmt :: remainingStmts))                                                                                                                                         {--)--}
                            (headLLines ++ ys ++ lLLines)                                                                                                                                               {--)--}
                            (headRLines ++ zs ++ rLLines)                                                                                                                                               {--)--}
                            (headCLines ++ ws ++ cLLines)                                                                                                                                               {--)--}
                            fullPrf)                                                                                                                                                                    {--)--}
            (Yes                                                                                                                                                                                        {--)--}
                (MkCSRListContaner                                                                                                                                                                      {--)--}
                    (S timeStep)                                                                                                                                                                        {--)--}
                    []                                                                                                                                                                                  {--)--}
                    []                                                                                                                                                                                  {--)--}
                    []                                                                                                                                                                                  {--)--}
                    []                                                                                                                                                                                  {--)--}
                    (WriteEmptyStatements (S timeStep)))) =>                                                                                                                                            {--)--}
                let fullPrf = (WriteStatements                                                                                                                                                          {--)--}
                                timeStep                                                                                                                                                                {--)--}
                                x                                                                                                                                                                       {--)--}
                                []                                                                                                                                                                      {--)--}
                                theStatement                                                                                                                                                            {--)--}
                                (WriteEmptyStatements (S timeStep))) in                                                                                                                                 {--)--}
                    Yes                                                                                                                                                                                 {--)--}
                        (MkCSRListContaner                                                                                                                                                              {--)--}
                            timeStep                                                                                                                                                                    {--)--}
                            (x :: [])                                                                                                                                                                   {--)--}
                            (headLLines ++ [])                                                                                                                                                          {--)--}
                            (headRLines ++ [])                                                                                                                                                          {--)--}
                            (headCLines ++ [])                                                                                                                                                          {--)--}
                            fullPrf)                                                                                                                                                                    {--)--}
            (No contra) => No (cantWriteRemaining contra theStatement)                                                                                                                                  {--)--}
    (No headContra) => No (cantWriteHead headContra)                                                                                                                                                    {--)--}
