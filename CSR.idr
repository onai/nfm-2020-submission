                              SExprCSRListContainer curVarNo timeStep sexprList exprBindings depth sexpr                                                                                                {--)--}
                                                                                                                                                                                                        {--)--}
                                                                                                                                                                                                        {--)--}
                                                                                                                                                                                                        {--)--}
cantWriteStatements : (statementContra : SExprToOperatorReturnType curVarNo sexpr sexprList exprBindings depth -> Void) ->                                                                              {--)--}
                      SExprCSRListContainer curVarNo timeStep sexprList exprBindings depth sexpr ->                                                                                                     {--)--}
                      Void                                                                                                                                                                              {--)--}
cantWriteStatements statementContra                                                                                                                                                                     {--)--}
                    (MkSExprCSRListContainer curVarNo                                                                                                                                                   {--)--}
                                             timeStep                                                                                                                                                   {--)--}
                                             sexpr                                                                                                                                                      {--)--}
                                             sexprList                                                                                                                                                  {--)--}
                                             exprBindings                                                                                                                                               {--)--}
                                             depth                                                                                                                                                      {--)--}
                                             statements                                                                                                                                                 {--)--}
                                             sexprWritten                                                                                                                                               {--)--}
                                             retVal                                                                                                                                                     {--)--}
                                             lLines                                                                                                                                                     {--)--}
                                             rLines                                                                                                                                                     {--)--}
                                             cLines                                                                                                                                                     {--)--}
                                             csrWritten                                                                                                                                                 {--)--}
                                             csrContainer                                                                                                                                               {--)--}
                                             csrLinesWritten) =                                                                                                                                         {--)--}
    statementContra retVal                                                                                                                                                                              {--)--}
                                                                                                                                                                                                        {--)--}
cantWriteCSRList : (prfLTE1 : LT 0 nextVarNo) ->                                                                                                                                                        {--)--}
    (writeListContra : CSRListContainer timeStep statements -> Void) ->                                                                                                                                 {--)--}
    (opSeqType : OperationSeq sexpr curVarNo exprBindings sexprList statements nextVarNo depth) ->                                                                                                      {--)--}
    SExprCSRListContainer curVarNo timeStep sexprList exprBindings depth sexpr ->                                                                                                                       {--)--}
    Void                                                                                                                                                                                                {--)--}
cantWriteCSRList prfLTE1                                                                                                                                                                                {--)--}
                 writeListContra                                                                                                                                                                        {--)--}
                 opSeqType                                                                                                                                                                              {--)--}
                 (MkSExprCSRListContainer curVarNo                                                                                                                                                      {--)--}
                                          timeStep                                                                                                                                                      {--)--}
                                          sexpr                                                                                                                                                         {--)--}
                                          sexprList                                                                                                                                                     {--)--}
                                          exprBindings                                                                                                                                                  {--)--}
                                          depth                                                                                                                                                         {--)--}
                                          xs                                                                                                                                                            {--)--}
                                          sexprWritten                                                                                                                                                  {--)--}
                                          retVal                                                                                                                                                        {--)--}
                                          lLines                                                                                                                                                        {--)--}
                                          rLines                                                                                                                                                        {--)--}
                                          cLines                                                                                                                                                        {--)--}
                                          csrWritten                                                                                                                                                    {--)--}
                                          csrContainer                                                                                                                                                  {--)--}
                                          csrLinesWritten)                                                                                                                                              {--)--}
                 {statements = statements} =                                                                                                                                                            {--)--}
    case sameOperationSeqFromSameSExpr sexprWritten opSeqType of                                                                                                                                        {--)--}
        (Refl, Refl, Refl) => writeListContra csrContainer                                                                                                                                              {--)--}
                                                                                                                                                                                                        {--)--}
sexprToCSRList : (curVarNo : Nat) ->                                                                                                                                                                    {--)--}
                 (timeStep : Nat) ->                                                                                                                                                                    {--)--}
                 (sexpr : SExpr) ->                                                                                                                                                                     {--)--}
                 (exprBindings : List (String, Nat)) ->                                                                                                                                                 {--)--}
                 (sexprList : SExprList) ->                                                                                                                                                             {--)--}
                 (depth : Nat) ->                                                                                                                                                                       {--)--}
                 Dec (SExprCSRListContainer curVarNo timeStep sexprList exprBindings depth sexpr)                                                                                                       {--)--}
sexprToCSRList curVarNo                                                                                                                                                                                 {--)--}
               timeStep                                                                                                                                                                                 {--)--}
               sexpr                                                                                                                                                                                    {--)--}
               exprBindings                                                                                                                                                                             {--)--}
               sexprList                                                                                                                                                                                {--)--}
               depth =                                                                                                                                                                                  {--)--}
    case (sexprToOperationSeq curVarNo sexpr sexprList exprBindings depth) of                                                                                                                           {--)--}
        (Yes (RetVal curVarNo sexpr sexprList exprBindings nextVarNo prfLTE1 statements depth opSeqType)) =>                                                                                            {--)--}
            case (writeCSRList timeStep statements) of                                                                                                                                                  {--)--}
                (Yes (MkCSRListContaner timeStep statements lLines rLines cLines writtenPrf)) =>                                                                                                        {--)--}
                    Yes                                                                                                                                                                                 {--)--}
                        (MkSExprCSRListContainer                                                                                                                                                        {--)--}
                            curVarNo                                                                                                                                                                    {--)--}
                            timeStep                                                                                                                                                                    {--)--}
                            sexpr                                                                                                                                                                       {--)--}
                            sexprList                                                                                                                                                                   {--)--}
                            exprBindings                                                                                                                                                                {--)--}
                            depth                                                                                                                                                                       {--)--}
                            statements                                                                                                                                                                  {--)--}
                            opSeqType                                                                                                                                                                   {--)--}
                            (RetVal curVarNo sexpr sexprList exprBindings nextVarNo prfLTE1 statements depth opSeqType)                                                                                 {--)--}
                            lLines                                                                                                                                                                      {--)--}
                            rLines                                                                                                                                                                      {--)--}
                            cLines                                                                                                                                                                      {--)--}
                            writtenPrf                                                                                                                                                                  {--)--}
                            (MkCSRListContaner timeStep statements lLines rLines cLines writtenPrf)                                                                                                     {--)--}
                            (WriteList curVarNo timeStep sexpr exprBindings sexprList opSeqType writtenPrf))                                                                                            {--)--}
                (No writeListContra) => No (cantWriteCSRList prfLTE1 writeListContra opSeqType)                                                                                                         {--)--}
        (No statementContra) => No (cantWriteStatements statementContra)                                                                                                                                {--)--}
                                                                                                                                                                                                        {--)--}
