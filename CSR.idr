                                                                                                                                                                                                        {--)--}
data StatementWritten : (timeStep : Nat) -> (statement : R1CSStatement) -> (lLines : List CSRLine) -> (rLines : List CSRLine) -> (cLines : List CSRLine) -> Type where                                  {--)--}
    WriteStatementPlus : (timeStep : Nat) ->                                                                                                                                                            {--)--}
                         (varNo : Nat) ->                                                                                                                                                               {--)--}
                         (leftVarNo : Nat) ->                                                                                                                                                           {--)--}
                         (rightVarNo : Nat) ->                                                                                                                                                          {--)--}
                         (StatementWritten                                                                                                                                                              {--)--}
                            timeStep                                                                                                                                                                    {--)--}
                            (Operation                                                                                                                                                                  {--)--}
                                varNo PLUS (VarLeaf leftVarNo) (VarLeaf rightVarNo))                                                                                                                    {--)--}
                            [(ConstElem timeStep 1)]                                                                                                                                                    {--)--}
                            [(CSRElem timeStep leftVarNo 1), (CSRElem timeStep rightVarNo 1)]                                                                                                           {--)--}
                            [(CSRElem timeStep varNo (- 1))])                                                                                                                                           {--)--}
                                                                                                                                                                                                        {--)--}
    WriteStatementMinus : (timeStep : Nat) ->                                                                                                                                                           {--)--}
                          (varNo : Nat) ->                                                                                                                                                              {--)--}
                          (leftVarNo : Nat) ->                                                                                                                                                          {--)--}
                          (rightVarNo : Nat) ->                                                                                                                                                         {--)--}
                          (StatementWritten                                                                                                                                                             {--)--}
                            timeStep                                                                                                                                                                    {--)--}
                            (Operation                                                                                                                                                                  {--)--}
                                varNo MINUS (VarLeaf leftVarNo) (VarLeaf rightVarNo))                                                                                                                   {--)--}
                            [(ConstElem timeStep 1)]                                                                                                                                                    {--)--}
                            [(CSRElem timeStep leftVarNo 1), (CSRElem timeStep rightVarNo (- 1))]                                                                                                       {--)--}
                            [(CSRElem timeStep varNo (- 1))])                                                                                                                                           {--)--}
                                                                                                                                                                                                        {--)--}
    WriteStatementMul : (timeStep : Nat) ->                                                                                                                                                             {--)--}
                        (varNo : Nat) ->                                                                                                                                                                {--)--}
                        (leftVarNo : Nat) ->                                                                                                                                                            {--)--}
                        (rightVarNo : Nat) ->                                                                                                                                                           {--)--}
                        (StatementWritten                                                                                                                                                               {--)--}
                           timeStep                                                                                                                                                                     {--)--}
                           (Operation                                                                                                                                                                   {--)--}
                               varNo MUL (VarLeaf leftVarNo) (VarLeaf rightVarNo))                                                                                                                      {--)--}
                           [(CSRElem timeStep leftVarNo 1)]                                                                                                                                             {--)--}
                           [(CSRElem timeStep rightVarNo 1)]                                                                                                                                            {--)--}
                           [(CSRElem timeStep varNo (- 1))])                                                                                                                                            {--)--}
                                                                                                                                                                                                        {--)--}
    WriteStatementPlusLiteral : (timeStep : Nat) ->                                                                                                                                                     {--)--}
                                (varNo : Nat) ->                                                                                                                                                        {--)--}
                                (leftDigit : Integer) ->                                                                                                                                                {--)--}
                                (rightVarNo : Nat) ->                                                                                                                                                   {--)--}
                                (StatementWritten                                                                                                                                                       {--)--}
                                  timeStep                                                                                                                                                              {--)--}
                                  (Operation                                                                                                                                                            {--)--}
                                    varNo PLUS (DigitLeaf leftDigit) (VarLeaf rightVarNo))                                                                                                              {--)--}
                                []                                                                                                                                                                      {--)--}
                                [(ConstElem timeStep leftDigit), (CSRElem timeStep rightVarNo 1)]                                                                                                       {--)--}
                                [(CSRElem timeStep varNo (- 1))])                                                                                                                                       {--)--}
                                                                                                                                                                                                        {--)--}
    WriteStatementPlusLiteral' : (timeStep : Nat) ->                                                                                                                                                    {--)--}
                                 (varNo : Nat) ->                                                                                                                                                       {--)--}
                                 (leftVarNo : Nat) ->                                                                                                                                                   {--)--}
                                 (rightDigit : Integer) ->                                                                                                                                              {--)--}
                                 (StatementWritten                                                                                                                                                      {--)--}
                                  timeStep                                                                                                                                                              {--)--}
                                  (Operation                                                                                                                                                            {--)--}
                                    varNo PLUS (VarLeaf leftVarNo) (DigitLeaf rightDigit))                                                                                                              {--)--}
                                  []                                                                                                                                                                    {--)--}
                                  [(ConstElem timeStep rightDigit), (CSRElem timeStep leftVarNo 1)]                                                                                                     {--)--}
                                  [(CSRElem timeStep varNo (- 1))])                                                                                                                                     {--)--}
                                                                                                                                                                                                        {--)--}
    WriteStatementMinusLiteral : (timeStep : Nat) ->                                                                                                                                                    {--)--}
                                 (varNo : Nat) ->                                                                                                                                                       {--)--}
                                 (leftVarNo : Nat) ->                                                                                                                                                   {--)--}
                                 (rightDigit : Integer) ->                                                                                                                                              {--)--}
                                 (StatementWritten                                                                                                                                                      {--)--}
                                    timeStep                                                                                                                                                            {--)--}
                                    (Operation                                                                                                                                                          {--)--}
                                        varNo MINUS (VarLeaf leftVarNo) (DigitLeaf rightDigit))                                                                                                         {--)--}
                                    []                                                                                                                                                                  {--)--}
                                    [(CSRElem timeStep leftVarNo 1), (ConstElem timeStep (- rightDigit))]                                                                                               {--)--}
                                    [(CSRElem timeStep varNo (- 1))])                                                                                                                                   {--)--}
                                                                                                                                                                                                        {--)--}
    WriteStatementMinusLiteral' : (timeStep : Nat) ->                                                                                                                                                   {--)--}
                                  (varNo : Nat) ->                                                                                                                                                      {--)--}
