pprintStatement (CSRElem row col value) = (show value) ++ " " ++ (show row) ++ " " ++ (show col)                                                                                                        {--)--}
pprintStatement (ConstElem timeStep value) = (show value) ++ " " ++ (show timeStep) ++ " " ++ (show Z)                                                                                                  {--)--}
